public class Solution {
    public int[] twoSum(int[] numbers, int target) {
        int i,j,len=numbers.length;
        int ans[]=new int[2];
        for(i=0;i<len;i++)
        {
        	for(j=i+1;j<len;j++)
        	{
        		if(numbers[i]+numbers[j]==target)
        		{
        			ans[0]=i+1;
        			ans[1]=j+1;
        			return ans;
        		}
        	}
        }
        return ans;
    }
}
